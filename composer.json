{
  "name": "fourfortymedia/eloquent-model-guard",
  "type": "library",
  "require": {
    "php": "^8.0",
    "laravel/framework": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "FourFortyMedia\\EloquentModelGuard\\": "src/",
      "FourFortyMedia\\Tests\\": "tests/",
      "FourFortyMedia\\Tests\\Models\\": "tests/Models"
    }
  },
  "require-dev": {
    "mockery/mockery": "^1.4.4",
    "phpunit/phpunit": "^8.5.19|^9.5.8",
    "orchestra/testbench": "^6.29",
    "nunomaduro/collision": "^5.11"
  },
  "authors": [
    {
      "name": "Ntiyiso Rikhotso",
      "email": "truminnd@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "scripts": {
    "post-autoload-dump": [
      "@php vendor/bin/testbench package:discover --ansi"
    ]
  }
}
